node ("J-VF1-1"){
  stage('*** Cleanup J-VF1-1 ***'){
    cleanWs()
  }
  stage ('*** 3s Sleep ***'){
    sleep(time:3,unit:"SECONDS")
  }
  stage('*** Checkout SCM J-VF1-1 ***'){
    retry(5){
      sh """
        git clone https://github.com/alitariq4589/coremark.git .
      """
    }
  }
  stage("*** Check GCC Version ***"){
    sh"""
      gcc --version
    """
  }
  stage('*** make J-VF1-1 ***'){
    sh"""#!/bin/bash
      make ITERATIONS=2000000 CC=gcc PORT_DIR=linux/ RUN=
    """
  }
  stage('*** Printing Results J-VF1-1 ***'){
    sh"""#!/bin/bash
      cat run1.log
    """
  }
}

